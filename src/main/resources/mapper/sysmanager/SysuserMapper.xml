<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.acme.jbp.sysmanager.mapper.SysuserMapper">

    <select id="getUserByUsername" parameterType="java.lang.String" resultType="com.acme.jbp.sysmanager.entity.Sysuser">
        select * from sysuser where username=#{username}
    </select>
</mapper>
